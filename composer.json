{
    "name": "bravist/laravel-district-explorer",
    "description": "District Explorer For Laravel with AutoNavi. 中国行政区划数据，访问高德在线数据获取最新区划数据。",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/console": "~5.6.0|~5.7.0|~5.8.0|~5.9.0",
        "illuminate/database": "~5.6.0|~5.7.0|~5.8.0|~5.9.0",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "程会勇",
            "email": "chenghuiyong1987@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bravist\\DistrictExplorer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bravist\\DistrictExplorer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bravist\\District\\DistrictExplorerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
